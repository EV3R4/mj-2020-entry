pico-8 cartridge // http://www.pico-8.com
version 27
__lua__
timer = 0
px = 2
py = 2
pa = 0
pspeed = 4
won = false
steps = 0
keys = 0
tels = {}
teleported = false
-- teleporter particle animation
-- tel        part     a
telparta = 0 -- unused
telpow = {}

sprdoor = 2
sprkey = 3
sprlevoff = 4
sprlevoffend = sprlevoff+7
sprlevon = sprlevoff+16
sprlevonend = sprlevon+7
sprenddoor = 28
sprchar = 16
sprtel = 32
sprtelend = sprtel+7
sprtelpart = sprtel+16

function cprint(t,y)
	print(t,64-#t*2,y)
end

function _init()
	for i=0,7 do
		tels[i] = {}
		telpow[i] = true
	end
	for y=0,62 do
		for x=0,46 do
			local t = mget(x,y)
			if t >= sprtel and
			   t <= sprtelend then
				local obj = {}
				obj.x = x
				obj.y = y
				obj.on = true
				add(tels[t-sprtel], obj)
			elseif t >= sprlevoff and
			    t <= sprlevoffend then
				telpow[t-sprlevoff] = false
			end
		end
	end
	
	music()
end

function _update()
	timer += 1
	
	if won then return end
	
	local nx = px
	local ny = py
	local ok = false
	if timer % pspeed == 0 then
		if btn(0) then nx -= 1 end
		if btn(1) then nx += 1 end
		if btn(2) then ny -= 1 end
		if btn(3) then ny += 1 end
	end
	local t = mget(nx,ny)
	if t < 1 then
		ok = true
		teleported = false
	elseif t == sprdoor then
		if keys > 0 then
			keys -= 1
			mset(nx,ny,0)
		 ok = true
		end
	elseif t == sprkey then
		keys += 1
		mset(nx,ny,0)
		ok = true
	elseif (t >= sprlevoff and
	  t <= sprlevoffend) or
	  (t >= sprlevon and
	  t <= sprlevonend) then
	 local nt,on
	 if t >= sprlevoff and
	    t <= sprlevoffend then
	 	nt = t+16
	 	on = true
	 else
	 	nt = t-16
	 	on = false
	 end
	 mset(nx,ny,nt)
	 
	 telpow[nt%16-4] = on
	elseif t == sprenddoor then
		mset(nx,ny,0)
		ok = true
		won = true
	elseif t >= sprtel and t <= sprtelend then
		local tel,tar
		local g = t-sprtel
		local t0 = tels[g][1]
		local t1 = tels[g][2]
		if t0.x == nx and
			  t0.y == ny then
			tel = t0
			tar = t1
		else
			tel = t1
			tar = t0
		end
		if not teleported and telpow[g] then
			nx = tar.x
			ny = tar.y
			ok = true
			teleported = true
		end
	end
	if ok and
				(px != nx or
				py != ny) then
		px = nx
		py = ny
		if pa == 0 then
			pa = 1
		else
			pa = 0
		end
		
		steps += 1
	end
end

function _draw()
	cls()
	
	local camx = flr(px/16)*16
	local camy = flr(py/16)*16
	
	for gid,g in pairs(tels) do
		for _,tel in pairs(g) do
			if telpow[gid] then spr(sprtelpart+flr(timer/8)%8,(tel.x-camx)*8,(tel.y-camy)*8) end
		end
	end
	
	map(camx,camy)
	
	spr(sprchar+pa,px%16*8,py%16*8)
	
	print(steps,12,3)
	
	spr(sprkey,2,8)
	print(keys,12,9)
	
	if won then
		rectfill(24,54,104,70,11)
		color(7)
		cprint("you win!",56)
		cprint("thanks for playing!",64)
	end
end
